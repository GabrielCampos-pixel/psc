import java.util.*;
public class Pesquisa {
    public static void main(String args[]) {
        Scanner teclado = new Scanner(System.in);
        int canal = 2, 4, 5, 7, 12;
        int audiencia = new int[canal.length];
        int totalPessoas = 0;
        System.out.println("digite o canal");
        int canal = teclado.nextInt();
        System.out.println("digite o numero de pessoas assistindo");
        int pessoas =teclado.nextInt();

        while(canal=2, 4, 5, 7, 12) {
            audiencia += pessoas;
            totalPessoas += pessoas;
            else(canal==0) {
                System.out.println("O percentual de cada canal[i] Ã© :" + audiencia[i]/totalPessoas*100); 
            }
        }
    }
}            